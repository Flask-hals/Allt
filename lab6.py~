
def sumlist(li):
    #print(li)
    if not li:
       return 0
    else:
       return li[0] + sumlist(li[1:])

print( sumlist([1,2,3,4,5]) )

def sum_arb_list(li):
    tot = 0
    for x in li:
        if not isinstance(x, list):
            tot += x #Om x inte Ã¤r en lista kan vi addera siffran till summan
        else:
            tot += sum_arb_list(x)
    return tot

tot=0
li = [3, [2, [3, 4], 5], 6, [7, 8]]
print(sum_arb_list(li))
tot=0
li = [4, 2, 3, 4, 5, 6, 7, 8]
print(sum_arb_list(li))

